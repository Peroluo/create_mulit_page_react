  "version": "0.0.2",
  "description": "multi_page_react",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env PROJECT=$npm_package_name   VERSION=$npm_package_version  webpack-dev-server  --mode development --devtool inline-source-map --hot",
    "build": "cross-env  PROJECT=$npm_package_name  BundleAnalyzer=BundleAnalyzer NODE_ENV=production VERSION=$npm_package_version webpack --mode production --watch --color --progress",
